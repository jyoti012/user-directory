
<h1>Listing Users</h1>
<%= link_to 'New', new_user_path() %>
 
<table>
  <tr>
    <th>Firstname</th>
    <th>Lastname</th>
    <th>Username</th>
    <th>Email Id</th>
    <th>Age</th>
    <th>Action</th>
  </tr>
 
  <% @users.each do |user| %>
    <tr>
      <td><%= user.firstname %></td>
      <td><%= user.lastname %></td>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
      <td><%= user.age %></td>
      <td><%= link_to 'Show', user_path(user) %> | <%= link_to 'Edit', edit_user_path(user) %></td>
    </tr>
  <% end %>
</table>